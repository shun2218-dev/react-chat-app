{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "tsc && vite build && firebase deploy",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test": "playwright test --trace on && playwright show-report",
    "snapshot-test": "playwright test --update-snapshots && playwright show-report",
    "test:codegen": "playwright codegen https://chat-app-4a684.web.app/",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "npx eslint --fix src",
    "fix": "npm run format && npm run lint"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.7",
    "express": "^4.18.1",
    "firebase": "^9.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.1",
    "react-router-dom": "^6.4.1",
    "react-use": "^17.4.0",
    "recoil": "^0.7.5",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.27.1",
    "@playwright/test": "^1.27.1",
    "@types/express": "^4.17.14",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "playwright-cli": "^0.180.0",
    "prettier": "^2.8.4",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  }
}
